{
  "schemaVersion": 1,
  "id": "hooked",
  "version": "${version}",

  "name": "Hooked",
  "description": "Hooked: Grappling hooks done simple, natural, and useful.",
  "authors": [
    "thecodewarrior"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/hooked",
    "sources": "https://github.com/thecodewarrior/Hooked",
    "issues": "https://github.com/thecodewarrior/Hooked/issues",
    "discord": "https://discord.gg/wsk2PBR"
  },

  "license": "",
  "icon": "hooked_logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.thecodewarrior.hooked.Hooked\u0024CommonInitializer"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.thecodewarrior.hooked.Hooked\u0024ClientInitializer"
      }
    ],
    "cardinal-components": [
      {
        "adapter": "kotlin",
        "value": "dev.thecodewarrior.hooked.Hooked\u0024Components"
      }
    ]
  },
  "mixins": [
    "hooked.mixins.json"
  ],

  "depends": {
    "fabric": "*",
    "fabric-language-kotlin": ">=1.9.1+kotlin.1.8.10",
    "minecraft": "1.19.2",
    "java": ">=16",
    "liblib-core": ">=5.0.0-alpha.10",
    "liblib-glitter": ">=5.0.0-alpha.10",
    "liblib-etcetera": ">=5.0.0-alpha.10",
    "liblib-mosaic": ">=5.0.0-alpha.10",
    "trinkets": ">=3.4.0",
    "fabric-key-binding-api-v1": "*"
  },
  "custom": {
    "cardinal-components": [
      "hooked:hook_data"
    ]
  }
}